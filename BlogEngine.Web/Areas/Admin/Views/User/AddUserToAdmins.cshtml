@model BlogEngine.Core.ViewModels.UserViewModel

@{
    ViewBag.Title = "Add User To Admins";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h2>Add user to admin role</h2>

@using (Html.BeginForm())
{
    @Html.LabelFor(model => model.users)
    @Html.DropDownListFor(model => model.SelectedUser, Model.users)
    
    <input type="submit" value="Submit" />
   
}
